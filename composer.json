{
  "name": "engtuncay/phputils8",
  "description": "OrakSoft Php Utility Library (Php 8)",
  "type": "library",
  "version": "0.0.48",
  "require": {
    "php": ">=8.1",
    "ext-pdo": "*",
    "phpoffice/phpspreadsheet": "^4.5",
    "monolog/monolog": "^3.8.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Engtuncay\\Phputils8\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Tuncay Orak",
      "email": "tuncayorak@gmail.com"
    }
  ],
  "archive": {
    "exclude": [
      ".git",
      ".gitignore",
      ".gitattributes",
      "tests",
      "phpunit.xml.dist",
      "*.md",
      "readme.md",
      "fi-logs"
    ]
  },
  "scripts": {
    "fi-cmp-install": "composer install",
    "fi-cmp-update": "composer update",
    "fi-cmp-dump-optimize": ["composer dump-autoload -o"],
    "fi-cmp-install-dev": ["composer install --dev"],
    "phpcs": "vendor/bin/phpcs --standard=phpcs.xml",
    "test": "phpunit"
  }
}
